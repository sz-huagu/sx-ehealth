<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.health.citymodel.mapper.CityMapper">
	<select id="findAllCity"  parameterType="City" resultType="City">
		SELECT
			t.id,
			t.`name`,
			t.province_id
		FROM
			t_city t
	</select>
</mapper>